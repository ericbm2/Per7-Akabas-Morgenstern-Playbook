class Coordinate{

  int x,y;
  
  Coordinate(int a, int b){
    x = a;
    y = b;
  }
  
  int getY(){
    return y;
  }
  
  int getX(){
    return x;
  }
  
  void setY(int a){
    y = a;
  }
    
  void setX(int b){
    x = b;
  }
  
  void setXY(int a, int b){
    x = a;
    y = b;
  }
  
  void setup(){}
  void draw(){}
  
}
