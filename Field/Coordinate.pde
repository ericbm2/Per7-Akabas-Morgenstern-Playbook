class Coordinate{

  double x,y;
  
  Coordinate(double a, double b){
    x = a;
    y = b;
  }
  
  double getY(){
    return y;
  }
  
  double getX(){
    return x;
  }
  
  void setY(double a){
    y = a;
  }
    
  void setX(double b){
    x = b;
  }
  
  void setXY(double a, double b){
    x = a;
    y = b;
  }
  
  void setup(){}
  void draw(){}
  
}
  
